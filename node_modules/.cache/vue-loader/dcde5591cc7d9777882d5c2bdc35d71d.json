s=\"formbox\">\r\n        <div>\r\n          <label for=\"name\">账号</label>\r\n          <input v-model=\"formCustom.name\" type=\"text\" id=\"name\" placeholder=\"请输入账号\" auto-complete=\"off\">\r\n        </div>\r\n        <div>\r\n          <label for=\"password\">密码</label>\r\n          <input v-model=\"formCustom.password\" type=\"password\" id=\"password\" placeholder=\"请输入密码\" auto-complete=\"off\">\r\n        </div>\r\n      </div>\r\n      <el-checkbox v-model=\"checked\" checked class=\"remember\" >记住密码</el-checkbox>\r\n      <button class=\"submit animate03\" @click=\"loginSubmit\">登 录</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {mapActions, mapGetters} from \"vuex\"\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      show: true,\r\n      checked: true,\r\n      formCustom: {\r\n        name: '',\r\n        password: '',\r\n        type: 'name'\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapGetters([\r\n      'user'\r\n    ])\r\n  },\r\n  created() {\r\n  },\r\n  methods: {\r\n    ...mapActions([\r\n      'Token', 'UserInfo'\r\n    ]),\r\n    loginSubmit() {\r\n      this.formCustom.name.indexOf('@') == -1 ? this.formCustom.type = 'name' : this.formCustom.type = 'email'\r\n      this.$post('/api/v2/admin/login', this.formCustom).then(res => {\r\n        // 因为接口请求用户信息需要时间，路由会先拦截，所以先设置 admin=true\r\n        let user = {\r\n          admin: true\r\n        }\r\n        this.$store.commit('USERINFO', user)\r\n        \r\n        this.$message.success('登录成功！');\r\n        this.Token(res.data.token)\r\n        this.UserInfo()\r\n\r\n        if (this.$route.query.redirect) {\r\n          this.$router.push(this.$route.query.redirect)\r\n        } else {\r\n          this.$router.push('/articlelist')\r\n        }\r\n      }).catch(() => {\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style scoped lang=\"stylus\">\r\n.animate03{\r\n  -webkit-transition-duration:0.3s;\r\n   -moz-transition-duration:0.3s;\r\n  